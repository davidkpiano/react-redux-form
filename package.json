{
  "name": "redux-simple-form",
  "version": "0.1.5",
  "description": "Simple Redux Forms",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run build",
    "test": "mocha --require babel-core/register --require ./test/spec-setup.js",
    "test:watch": "mocha -w --require babel-core/register --require ./test/spec-setup.js",
    "build": "npm run build:umd && npm run build:lib",
    "build:link": "babel -w src --out-dir lib",
    "build:watch": "webpack -w src/index.js dist/index.js --config webpack.config.base.js",
    "build:umd": "webpack -p src/index.js dist/index.js --config webpack.config.prod.js",
    "build:lib": "babel src --out-dir lib",
    "preversion": "npm run test",
    "prepublish": "npm test && npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidkpiano/redux-simple-form.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidkpiano/redux-simple-form/issues"
  },
  "homepage": "https://github.com/davidkpiano/redux-simple-form#readme",
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "chai-subset": "^1.2.0",
    "enzyme": "^1.2.0",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.6",
    "redux-thunk": "^1.0.3",
    "should": "^8.0.2",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "lodash": "^4.0.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-ignore": "^1.2.1",
    "seamless-immutable": "^5.0.1"
  }
}
